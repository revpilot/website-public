{
  "name": "salessensei",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "run-p build:**",
    "build:lambda": "netlify-lambda build lambda -c ./webpack.config.js",
    "build:app": "nuxt build",
    "start": "nuxt start",
    "generatenobuild": "nuxt generate --no-build",
    "generate": "nuxt generate",
    "generate:app": "nuxt generate",
    "generate:lambda": "netlify-lambda build lambda -c ./webpack.config.js",
    "dev": "npm-run-all --parallel dev:app dev:lambda",
    "dev:app": "nuxt",
    "dev:lambda": "netlify-lambda serve lambda -c ./webpack.config.js"
  },
  "dependencies": {
    "@nuxt/content": "^1.13.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/prismic": "^1.2.4",
    "@nuxtjs/pwa": "^3.3.5",
    "@sendgrid/mail": "^7.4.2",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.9.0",
    "dotenv": "^8.2.0",
    "jquery": "^3.5.1",
    "netlify-lambda": "^2.0.3",
    "nuxt": "^2.15.1",
    "prismic-dom": "^2.2.5",
    "prismic-javascript": "^3.0.2",
    "prismic-vue": "^2.0.1",
    "vue-cookie-law": "^1.13.3",
    "vue-lodash": "^2.1.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "run-p": "0.0.0"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
